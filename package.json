{
  "name": "antigravity-quotas",
  "displayName": "Antigravity Quotas",
  "description": "Shows remaining quotas for Antigravity models",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "antigravity-quotas",
          "title": "Antigravity Quotas",
          "icon": "$(dashboard)"
        }
      ]
    },
    "views": {
      "antigravity-quotas": [
        {
          "id": "quota-view",
          "name": "Model Quotas"
        }
      ]
    },
    "commands": [
      {
        "command": "quota-view.refreshEntry",
        "title": "Refresh Quotas",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "quota-view.refreshEntry",
          "when": "view == quota-view",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "18.x",
    "typescript": "^5.1.3",
    "@vscode/vsce": "^2.19.0"
  }
}
